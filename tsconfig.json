{
  "compilerOptions": {
    "types": [
      "./types.d.ts",
      "@types/chrome"
    ],
    "outDir": "lib",
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@images/*": [
        "public/images/*"
      ],
      "@icons/*": [
        "public/icons/*"
      ],
      "@logos/*": [
        "public/icons/logos/*"
      ],
      "@symbols/*": [
        "public/icons/symbols/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    },
    "moduleResolution": "bundler",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/base"
}